else
  export NODE_PATH="/mnt/c/Users/joshv/OneDrive - BYU-Idaho/2024/Spring 2024/cse499_senior_project/dk_ecommerce/node_modules/.pnpm/npm@10.8.1/node_modules/npm/node_modules/cssesc/bin/node_modules:/mnt/c/Users/joshv/OneDrive - BYU-Idaho/2024/Spring 2024/cse499_senior_project/dk_ecommerce/node_modules/.pnpm/npm@10.8.1/node_modules/npm/node_modules/cssesc/node_modules:/mnt/c/Users/joshv/OneDrive - BYU-Idaho/2024/Spring 2024/cse499_senior_project/dk_ecommerce/node_modules/.pnpm/npm@10.8.1/node_modules/npm/node_modules:/mnt/c/Users/joshv/OneDrive - BYU-Idaho/2024/Spring 2024/cse499_senior_project/dk_ecommerce/node_modules/.pnpm/npm@10.8.1/node_modules:/mnt/c/Users/joshv/OneDrive - BYU-Idaho/2024/Spring 2024/cse499_senior_project/dk_ecommerce/node_modules/.pnpm/node_modules:$NODE_PATH"
